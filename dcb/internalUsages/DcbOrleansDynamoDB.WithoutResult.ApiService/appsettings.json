{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Amazon": "Warning",
      "Amazon.Runtime": "Warning",
      "Orleans": "Warning",
      "Orleans.Streams": "Warning",
      "DcbOrleansDynamoDB.WithoutResult.ApiService": "Information"
    }
  },
  "AllowedHosts": "*",
  "Sekiban": {
    "Database": "dynamodb"
  },
  "AWS": {
    "Region": "us-east-1"
  },
  "DynamoDb": {
    "EventsTableName": "SekibanEvents",
    "TagsTableName": "SekibanTags",
    "ProjectionStatesTableName": "SekibanProjectionStates"
  },
  "S3BlobStorage": {
    "BucketName": "sekiban-snapshots",
    "EnableEncryption": true
  }
}
