{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Amazon": "Warning",
      "Amazon.Runtime": "Warning",
      "Orleans": "Warning",
      "Orleans.Streams": "Warning",
      "SekibanDcbOrleansAws.ApiService": "Information"
    }
  },
  "AllowedHosts": "*",
  "Sekiban": {
    "Database": "dynamodb"
  },
  "Orleans": {
    "UseInMemoryStreams": true
  },
  "DynamoDb": {
    "EventsTableName": "sekibandcborleansaws-events",
    "TagsTableName": "sekibandcborleansaws-events-tags",
    "ProjectionStatesTableName": "sekibandcborleansaws-events-projections"
  },
  "S3BlobStorage": {
    "BucketName": "sekibandcborleansaws-snapshots"
  }
}
