{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Azure.Core": "Warning",
      "Azure.Identity": "Warning",
      "Azure.Storage": "Warning",
      "Azure.Messaging.EventHubs": "Warning",
      "Orleans.Runtime": "Warning",
      "Orleans.Streams": "Warning",
      "Orleans.Streaming": "Warning"
    }
  },
  "ApplicationInsights": {
    "EnableAdaptiveSampling": true,
    "InitialSamplingPercentage": 20.0,
    "MinSamplingPercentage": 1.0,
    "SamplingSettings": {
      "IsEnabled": true,
      "MaxTelemetryItemsPerSecond": 0.2,
      "ExcludedTypes": "Trace;Event"
    }
  },
  "AllowedHosts": "*"
}
